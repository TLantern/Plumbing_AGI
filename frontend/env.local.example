# Frontend Environment Configuration for Local Development
# Copy this to .env.local for local development

# Clerk Authentication Configuration (Frontend)
# For local development - use test keys from Clerk dashboard
VITE_CLERK_PUBLISHABLE_KEY=your_test_clerk_publishable_key
VITE_CLERK_SECRET_KEY=your_test_clerk_secret_key

# Supabase Configuration
VITE_SUPABASE_PROJECT_ID=your_supabase_project_id
VITE_SUPABASE_PUBLISHABLE_KEY=your_supabase_anon_key
VITE_SUPABASE_URL=https://your-supabase-project.supabase.co

# Legacy NextAuth (if needed)
NEXTAUTH_URL=http://localhost:8080
NEXTAUTH_SECRET=your_secret_key_for_local_development
AUTH_USERNAME=admin
AUTH_PASSWORD=admin
